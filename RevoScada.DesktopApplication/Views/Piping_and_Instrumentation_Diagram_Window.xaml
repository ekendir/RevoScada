                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="150"
                                VerticalAlignment="Bottom"
                                Text="General Pneumatic" />

                            <Button
                                x:Name="pneumatic"
                                Width="42"
                                Height="32"
                                Style="{StaticResource furnaceGeneralSwitchStatus}"
                                Tag="{Binding PipingAndInstrumentationFurnaceControlModel.GeneralPnomaticStatus, Converter={StaticResource IntToString}}" />

                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="150"
                                VerticalAlignment="Bottom"
                                Text="Hydrolic Confirmation" />

                            <Button
                                x:Name="hydrolicConfirm"
                                Width="42"
                                Height="32"
                                Style="{StaticResource furnaceGeneralSwitchStatus}"
