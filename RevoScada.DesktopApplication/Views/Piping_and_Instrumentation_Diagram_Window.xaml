                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="150"
                                VerticalAlignment="Bottom"
                                Text="Circulation Water" />

                            <Button
                                x:Name="CirculationWater"
                                Width="42"
                                Height="32"
                                Style="{StaticResource furnaceGeneralSwitchStatus}"
                                Tag="{Binding PipingAndInstrumentationFurnaceControlModel.CirculationWaterStatus, Converter={StaticResource IntToString}}" />

                        </StackPanel>
                    </StackPanel>
                </Border>
