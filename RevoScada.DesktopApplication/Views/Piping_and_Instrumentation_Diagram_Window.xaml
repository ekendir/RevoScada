                        <TextBlock
                            FontSize="16"
                            FontWeight="Bold"
                            Text="Part Temperature (°C)" />
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="Set Point" />
                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.AirTcSetPoint, StringFormat={}{0:F2}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="Actual" />
                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.AirTcActualValue, StringFormat={}{0:F0}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="High PTC" />
                            <Label
                                HorizontalAlignment="Left"
                                Content="{Binding PipingAndInstrumentationFurnaceControlModel.HighPtcPortNumber}"
                                ContentStringFormat="PTC - {0}" />
                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.HighPtcPortValue, StringFormat={}{0:F0}}" />

                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="Low PTC" />
                            <Label
                                HorizontalAlignment="Left"
                                Content="{Binding PipingAndInstrumentationFurnaceControlModel.LowPtcPortNumber}"
