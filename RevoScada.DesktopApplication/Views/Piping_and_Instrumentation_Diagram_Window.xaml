                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.LowPtcPortValue, StringFormat={}{0:F0}}" />
                        </StackPanel>
                    </StackPanel>
                </Border>
