<Window
    x:Class="RevoScada.DesktopApplication.Views.Piping_and_Instrumentation_Diagram_Window"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:arrows="clr-namespace:RevoScada.DesktopApplication.Helpers.ArrowHelper"
    xmlns:converters="clr-namespace:RevoScada.DesktopApplication.Converters"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
    xmlns:local="clr-namespace:RevoScada.DesktopApplication.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="P&amp;ID"
    Width="1920"
    Height="1080"
    Background="{StaticResource LightGrayColor}"
    Loaded="Window_Loaded"
    Unloaded="Window_Unloaded"
    WindowState="Maximized"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.Resources>
        <!--#region Converters-->
        <converters:ScrollBarVisibilityToMarginConverter x:Key="ScrollBarVisibilityToMarginConverter" />
        <converters:AllBoolToEnabled x:Key="AllBoolToEnabled" />
        <converters:IntToString x:Key="IntToString" />
        <!--#endregion-->

        <Style TargetType="TextBlock">
            <Setter Property="FontWeight" Value="SemiBold" />
            <Setter Property="Foreground" Value="{StaticResource CustomBlackColor}" />
            <Setter Property="FontSize" Value="14" />
            <Setter Property="Margin" Value="0,3,0,0" />
        </Style>
        <Style x:Key="lightTb" TargetType="TextBlock">
            <Setter Property="FontWeight" Value="SemiBold" />
            <Setter Property="Foreground" Value="{StaticResource GeneralWhite}" />
            <Setter Property="FontSize" Value="14" />
        </Style>
        <Style x:Key="TurkishDesc" TargetType="TextBlock">
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="FontSize" Value="15" />
            <Setter Property="Foreground" Value="{StaticResource pAndIBlue}" />
        </Style>
        <Style x:Key="blackChromeArrow" TargetType="{x:Type arrows:ArrowLine}">
            <Setter Property="Stroke" Value="{StaticResource pAndIGray}" />
            <Setter Property="StrokeThickness" Value="3" />
        </Style>
        <Style x:Key="brownArrow" TargetType="{x:Type arrows:ArrowLine}">
            <Setter Property="Stroke" Value="{StaticResource pAndIBrown}" />
            <Setter Property="StrokeThickness" Value="3" />
        </Style>
        <Style x:Key="blueArrow" TargetType="{x:Type arrows:ArrowLine}">
            <Setter Property="Stroke" Value="{StaticResource pAndIBlue}" />
            <Setter Property="StrokeThickness" Value="3" />
        </Style>
        <Style x:Key="greenArrow" TargetType="{x:Type arrows:ArrowLine}">
            <Setter Property="Stroke" Value="{StaticResource pAndIGreen}" />
            <Setter Property="StrokeThickness" Value="3" />
        </Style>
        <Style x:Key="yellowArrow" TargetType="{x:Type arrows:ArrowLine}">
            <Setter Property="Stroke" Value="{StaticResource pAndIYellow}" />
            <Setter Property="StrokeThickness" Value="3" />
        </Style>
        <Style x:Key="whiteTextBoxDX" TargetType="dxe:TextEdit">
            <Setter Property="Width" Value="100" />
            <Setter Property="Height" Value="30" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{StaticResource CustomBlackColor}" />
            <Setter Property="Background" Value="{StaticResource GeneralWhite}" />
            <Setter Property="FontFamily" Value="Verdana" />
            <Setter Property="FontSize" Value="11.5" />
            <Setter Property="FontWeight" Value="SemiBold" />
        </Style>
        <Style x:Key="whiteTextBoxDXWithBd" TargetType="dxe:TextEdit">
            <Setter Property="Width" Value="100" />
            <Setter Property="Height" Value="30" />
            <Setter Property="BorderBrush" Value="{StaticResource CustomBlackColor}" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Foreground" Value="{StaticResource CustomBlackColor}" />
            <Setter Property="Background" Value="{StaticResource GeneralWhite}" />
            <Setter Property="FontFamily" Value="Verdana" />
            <Setter Property="FontSize" Value="11.5" />
            <Setter Property="BorderTemplate">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type ContentControl}">
                        <Border
                            x:Name="border"
                            Background="{Binding RelativeSource={RelativeSource AncestorType=dxe:TextEdit}, Path=Background}"
                            BorderBrush="{Binding RelativeSource={RelativeSource AncestorType=dxe:TextEdit}, Path=BorderBrush}"
                            BorderThickness="{Binding RelativeSource={RelativeSource AncestorType=dxe:TextEdit}, Path=BorderThickness}">
                            <ContentPresenter Margin="0,0,-1,0" />
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <Style x:Key="autoManualBorderValue" TargetType="{x:Type Border}">
            <Setter Property="Width" Value="16" />
            <Setter Property="Height" Value="16" />
            <Setter Property="Background" Value="{StaticResource pAndIBlue}" />
            <Setter Property="BorderBrush" Value="{StaticResource CustomBlackColor}" />
            <Setter Property="BorderThickness" Value="1" />
        </Style>


        <Style x:Key="txtBlockValue" TargetType="{x:Type TextBlock}">
            <Setter Property="Text" Value="A" />
            <Setter Property="Width" Value="16" />
            <Setter Property="Height" Value="16" />
            <Setter Property="HorizontalAlignment" Value="Center" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="FontSize" Value="11" />
            <Setter Property="FontWeight" Value="SemiBold" />
            <Setter Property="Background" Value="{StaticResource pAndIBlue}" />
            <Setter Property="Foreground" Value="{StaticResource GeneralWhite}" />
            <Style.Triggers>
                <DataTrigger Binding="{Binding ElementName=isSystemAuto, Path=IsChecked}" Value="True">
                    <Setter Property="Text" Value="A" />
                    <Setter Property="Padding" Value="4,0,1,1" />
                    <Setter Property="Width" Value="16" />
                </DataTrigger>
                <DataTrigger Binding="{Binding ElementName=isSystemAuto, Path=IsChecked}" Value="False">
                    <Setter Property="Text" Value="M" />
                    <Setter Property="Padding" Value="4,0,1,1" />
                    <Setter Property="Width" Value="16" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <Grid>
        <!--#region Close Button-->
        <Button
            x:Name="exitBtn"
            Margin="{Binding ElementName=scrollViewer, Path=ComputedVerticalScrollBarVisibility, Converter={StaticResource ScrollBarVisibilityToMarginConverter}}"
            HorizontalAlignment="Right"
            VerticalAlignment="Top"
            Panel.ZIndex="10"
            Click="exitBtn_Click"
            Style="{StaticResource pipingAndInstrumentExitBtn}" />
        <!--#endregion-->

        <ScrollViewer
            x:Name="scrollViewer"
            HorizontalScrollBarVisibility="Auto"
            VerticalScrollBarVisibility="Auto">
            <!--#region Canvas-->
            <Canvas
                x:Name="mainCanvas"
                Width="1920"
                Height="1080">
                <TextBlock
                    Canvas.Left="165"
                    Canvas.Top="10"
                    Margin="0,0,0,10"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    FontWeight="SemiBold"
                    Foreground="Black"
                    Text="Compresor" />
                <Image
                    x:Name="compresor"
                    Canvas.Left="165"
                    Canvas.Top="30"
                    Width="87"
                    Height="100"
                    Source="pack://siteoforigin:,,,/Resources/Simple_tank_01.png" />
                <arrows:ArrowLine
                    Canvas.Left="250"
                    Canvas.Top="75"
                    ArrowEnds="None"
                    Style="{StaticResource blueArrow}"
                    StartPoint="1,10"
                    EndPoint="120,10" />



                <arrows:ArrowLine
                    Canvas.Left="208"
                    Canvas.Top="120"
                    ArrowEnds="None"
                    Style="{StaticResource blueArrow}"
                    StartPoint="1,10"
                    EndPoint="1,190" />
                <Button
                    x:Name="compressorValve"
                    Canvas.Left="182"
                    Canvas.Top="142"
                    Click="generalValveOnOff_ShowControl_Click"
                    Command="{Binding PressureSupplyControlCommand}"
                    Style="{StaticResource onOffValveWithActuatorBtn}"
                    Tag="{Binding PipingAndInstrumentationFurnaceControlModel.CompressorValveStatus, Converter={StaticResource IntToString}}">
                    <Button.LayoutTransform>
                        <RotateTransform Angle="90" CenterX="0.5" CenterY="0.5" />
                    </Button.LayoutTransform>
                </Button>
                <CheckBox
                    x:Name="isSystemAuto1"
                    IsChecked="{Binding PipingAndInstrumentationFurnaceControlModel.PressureSupplySelectionValveAutoManual}"
                    Visibility="Collapsed" />
                <TextBlock Canvas.Left="252" Canvas.Top="132">
                    <TextBlock.Style>
                        <Style TargetType="{x:Type TextBlock}">
                            <Setter Property="Width" Value="16" />
                            <Setter Property="Height" Value="16" />
                            <Setter Property="TextAlignment" Value="Left" />
                            <Setter Property="FontSize" Value="11" />
                            <Setter Property="FontWeight" Value="SemiBold" />
                            <Setter Property="Background" Value="{StaticResource pAndIBlue}" />
                            <Setter Property="Foreground" Value="{StaticResource GeneralWhite}" />
                            <Style.Triggers>
                                <DataTrigger Binding="{Binding ElementName=isSystemAuto1, Path=IsChecked}" Value="True">
                                    <Setter Property="Text" Value="A" />
                                    <Setter Property="Padding" Value="4,0,1,1" />
                                </DataTrigger>
                                <DataTrigger Binding="{Binding ElementName=isSystemAuto1, Path=IsChecked}" Value="False">
                                    <Setter Property="Text" Value="M" />
                                    <Setter Property="Padding" Value="3,0,1,1" />
                                    <Setter Property="Width" Value="16" />
                                </DataTrigger>
                            </Style.Triggers>
                        </Style>
                    </TextBlock.Style>

                </TextBlock>

                <!--<TextBlock
                    Canvas.Left="202"
                    Canvas.Top="142"
                     Name="{Binding ElementName=isSystemAuto1, Path=IsChecked}"
                    Style="{StaticResource txtBlockValue}" />-->
                <TextBlock
