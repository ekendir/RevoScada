                    Style="{StaticResource onOffValveWithActuatorBtn}"
                    Tag="{Binding PipingAndInstrumentationFurnaceControlModel.BoosterValveStatus, Converter={StaticResource IntToString}}">
                    <Button.LayoutTransform>
