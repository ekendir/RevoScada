                <arrows:ArrowLine
                    Canvas.Left="410"
                    Canvas.Top="120"
                    ArrowEnds="None"
                    Style="{StaticResource blueArrow}"
                    StartPoint="1,10"
                    EndPoint="1,100" />
                <Button
                    x:Name="boosterValve"
                    Canvas.Left="384"
                    Canvas.Top="142"
