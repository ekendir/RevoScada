                        <TextBlock
                            FontSize="16"
                            FontWeight="Bold"
                            Text="General Furnace Switch Status" />
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="150"
                                VerticalAlignment="Bottom"
                                Text="Door Open" />
                            <Button
                                x:Name="doorOpen"
                                Width="42"
                                Height="32"
                                HorizontalAlignment="Left"
                                Style="{StaticResource furnaceGeneralSwitchStatus}"
                                Tag="{Binding PipingAndInstrumentationFurnaceControlModel.DoorOpenSwStatus, Converter={StaticResource IntToString}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="150"
                                VerticalAlignment="Bottom"
                                Text="Door Close" />
                            <Button
                                x:Name="doorClose"
                                Width="42"
                                Height="32"
                                Style="{StaticResource furnaceGeneralSwitchStatus}"
                                Tag="{Binding PipingAndInstrumentationFurnaceControlModel.DoorCloseSwStatus, Converter={StaticResource IntToString}}" />

                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="150"
                                VerticalAlignment="Bottom"
                                Text="Ring Open" />

                            <Button
