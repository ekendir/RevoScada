                    BorderBrush="{StaticResource CustomBlackColor}"
                    BorderThickness="1">
                    <StackPanel>
                        <TextBlock
                            FontSize="16"
                            FontWeight="Bold"
                            Text="Part Vacuum Header (Bar)" />
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="High VAS" />
                            <Label
                                HorizontalAlignment="Left"
                                Content="{Binding PipingAndInstrumentationFurnaceControlModel.HighMonPortNumber}"
                                ContentStringFormat="VAS - {0}" />
                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.HighMonPortValue, StringFormat={}{0:F0}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="Low VAS" />
                            <Label
                                HorizontalAlignment="Left"
                                Content="{Binding PipingAndInstrumentationFurnaceControlModel.LowMonPortNumber}"
                                ContentStringFormat="VAS - {0}" />
                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.LowMonPortValue, StringFormat={}{0:F0}}" />
                        </StackPanel>

                        <StackPanel Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                VerticalAlignment="Bottom"
                                Text="Set Point" />
                            <TextBlock Margin="10,5,0,0" Text="{Binding PipingAndInstrumentationFurnaceControlModel.VacSetPoint, StringFormat={}{0:F0}}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock
